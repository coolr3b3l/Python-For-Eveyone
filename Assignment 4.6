def computepay(h,r):
    if h >= 40:
        rate = r * 40 + (r * 1.5) * (h - 40)
        return rate
    elif h <= 40:
        return h * r
hrs = float(input("Enter Hours:"))
rate =  float(input("Enter Rate:"))
p = computepay(hrs,rate)
print(p)

